<div class="container">
  <div class="jumbotron">
    <h1>User Profile</h1>
    <p>Bootstrap is the most popular HTML, CSS, and JS framework for developing
      responsive, mobile-first projects on the web.</p>
  </div>
  <div class="profile-book-section">
    <h3>Purchased Books</h3>
    <ul class="list-group">
      {{#each books}}
      <li class="list-group-item">{{this.bookId.book_name}} --- purchase date: {{this.purchaseDate}} -- <a
          href="/book/return/{{this.bookId._id}}" class="btn btn-info">Return</a></li>
      {{/each}}
    </ul>
  </div>
</div>